<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@AppService.CompanyShortName</title>
    <meta content="" name="description">
    <meta content="" name="keywords">

    <link href="@Url.Content("~/Plugins/Company/assets/img/favicon.png")" rel="icon">
    <link href="@Url.Content("~/Plugins/Company/assets/img/apple-touch-icon.png")" rel="apple-touch-icon">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Roboto:300,300i,400,400i,500,500i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">
    <link href="@Url.Content("~/Plugins/Company/assets/vendor/animate.css/animate.min.css")" rel="stylesheet">
    <link href="@Url.Content("~/Plugins/Company/assets/vendor/aos/aos.css")" rel="stylesheet">
    <link href="@Url.Content("~/Plugins/SweetAlert/sweetalert2.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/bootstrap.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/all.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Plugins/Company/assets/vendor/bootstrap-icons/bootstrap-icons.css")" rel="stylesheet">
    <link href="@Url.Content("~/Plugins/Company/assets/vendor/boxicons/css/boxicons.min.css")" rel="stylesheet">
    <link href="@Url.Content("~/Plugins/Company/assets/vendor/glightbox/css/glightbox.min.css")" rel="stylesheet">
    <link href="@Url.Content("~/Plugins/Company/assets/vendor/remixicon/remixicon.css")" rel="stylesheet">
    <link href="@Url.Content("~/Plugins/Company/assets/vendor/swiper/swiper-bundle.min.css")" rel="stylesheet">
    <link href="@Url.Content("~/Plugins/Company/assets/css/style.css")" rel="stylesheet">
    @RenderSection("styles", false)
    <script src="@Url.Content("~/Plugins/SweetAlert/sweetalert2.min.js")"></script>
    @RenderSection("scripts", false)
</head>
<body>
    @Html.Partial("~/Views/PartialViews/_PartialHomeHeader.cshtml")

    <div>
        @Html.Partial("~/Views/PartialViews/_PartialErrorMessage.cshtml")
        @RenderBody()
    </div>

    @Html.Partial("~/Views/PartialViews/_PartialHomeFooter.cshtml")

    <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

    <script src="@Url.Content("~/Plugins/Company/assets/vendor/aos/aos.js")"></script>
    <script src="~/Scripts/bootstrap.bundle.min.js"></script>
    <script src="@Url.Content("~/Plugins/Company/assets/vendor/glightbox/js/glightbox.min.js")"></script>
    <script src="@Url.Content("~/Plugins/Company/assets/vendor/isotope-layout/isotope.pkgd.min.js")"></script>
    <script src="@Url.Content("~/Plugins/Company/assets/vendor/swiper/swiper-bundle.min.js")"></script>
    <script src="@Url.Content("~/Plugins/Company/assets/vendor/waypoints/noframework.waypoints.js")"></script>
    <script src="@Url.Content("~/Plugins/Company/assets/vendor/php-email-form/validate.js")"></script>
    <script src="@Url.Content("~/Plugins/Company/assets/js/main.js")"></script>
    @RenderSection("scriptsbody", false)
</body>
</html>
